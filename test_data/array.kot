import libc;

def sum(array: i64*, n: i64) -> i64 {
	let result = 0;
	for (let i = 0; i < n; i = i + 1) {
		result = result + array[i];
	}
	return result;
}

def main(argc: i64, argv: i8**) -> i64 {
	let array = [12, 33];
	printf("array[0]: %d\n", array[0]);
	printf("array[1]: %d\n", array[1]);
	printf("sum: %d\n", sum(array, 2));
	return 0;
}
